<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
    
        <record model="ir.ui.view" id="stock_dhl_ice_tree_view">
            <field name="name">stock.dhl.ice.tree</field>
            <field name="model">stock.dhl.ice</field>
            <field name="arch" type="xml">
